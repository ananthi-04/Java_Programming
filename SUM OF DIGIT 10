import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {     
         Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        if(n<100)
        {
            System.out.println("Invalid Input");
        } 
        else 
        {
            int sum = 0,t=n;
            while(t>0) {
                sum=sum+t%10;
                t=t/10;
            }
            System.out.println("Sum of digit is " + sum);
        }
    }
}
