import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
         Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        int sum = 0,t = num;
        while (t > 0) {
            sum =sum+t % 10;
            t =t/10;
        }
        if (num % sum == 0) {
            System.out.println("Harshad Number");
        } else {
            System.out.println("Not Harshad Number");
        }
    }
}
