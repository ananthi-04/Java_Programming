import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String s1=sc.nextLine().replace(" ","");
        String s2=sc.nextLine().replace(" ","");
        String result=s1+s2;
        System.out.println(result);
    }
}
