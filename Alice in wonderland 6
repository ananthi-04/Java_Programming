import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int val=sc.nextInt();
        int sum=(val/10)+(val%10);
        System.out.println("Bird said:" +val);
        System.out.println("Alice must go in path-"+sum);
        
    }
}
